    "version": "0.2.0",
    "configurations": [
        {
            "name": "(lldb) Launch",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/test/USB/dist/main",
            "args": [],
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "make CH376s USB debug/test"
        },
        {
            "name": "(lldb) Launch OpenMSX ETH",
            "type": "lldb",
            "request": "launch",
            "program": "../Emulators/OpenMSX/derived/x86_64-darwin-devel/bin/openmsx",
            "args": ["-machine", "Philips_NMS_8250_MSM", "-diska", "${workspaceFolder}/drivers/UsbEthernet/dist","-setting", "${workspaceFolder}/drivers/UsbEthernet/dist/fullspeed.xml"],
            //"args": ["-machine", "Panasonic_FS-A1GT", "-diska", "${workspaceFolder}/drivers/UsbEthernet/dist","-setting", "${workspaceFolder}/drivers/UsbEthernet/dist/fullspeed.xml"],
            "cwd": "../Emulators/OpenMSX/share"
        },
        {
            "name": "(lldb) Launch OpenMSX KBD",
            "type": "lldb",
            "request": "launch",
            "program": "../Emulators/OpenMSX/derived/x86_64-darwin-devel/bin/openmsx",
            "args": ["-machine", "Philips_NMS_8250_MSM", "-cart", "${workspaceFolder}/drivers/UsbHost/dist/unapiusb.rom", "-diska", "${workspaceFolder}/drivers/UsbKeyboard/dist"],
            "cwd": "../Emulators/OpenMSX/share"
        }
    ]
